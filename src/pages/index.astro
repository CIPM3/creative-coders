---
import Layout from "../layouts/Layout.astro";
import { navModel } from "@hooks/navModel";

import Navbar from "@components/navbar/Navbar.astro";
import Heroes from "@components/main/heroes.astro";
import Services from "@components/main/servicios.astro";
import Proyects from "@components/main/proyects.astro";
import Packages from "@components/main/packages.astro";


const { title } = navModel;
---

<Layout title={`${title}`}>
  <!-- NAVBAR -->
  <Navbar />
  <!-- Heroes Section -->
  <Heroes />
  <!-- Services Section -->
  <Services />
  <!-- Proyects Section -->
  <Proyects />
  <!-- Packages Section -->
  <Packages/>
</Layout>
