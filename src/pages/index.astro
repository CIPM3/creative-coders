---
import Layout from "../layouts/Layout.astro";

import Navbar from "@components/navbar/Navbar.astro";
import Heroes from "@components/main/heroes.astro";
import Services from "@components/main/servicios.astro";
import Proyects from "@components/main/proyects.astro";
import Contact from "@components/main/contact.astro";
import Footer from "@components/main/footer.astro";

import RPackages from "@components/main/packages";


const title = "CreativeCoders - Home";
---

<Layout title={`${title}`}>
  <!-- NAVBAR -->
  <Navbar />
  <!-- Heroes Section -->
  <Heroes />
  <!-- Services Section -->
  <Services />
  <!-- Proyects Section -->
  <Proyects />
  <!-- Packages Section -->
  <!-- <Packages /> -->
  <RPackages client:only="react" />
  <!-- Contact Section -->
  <Contact />
  <!-- Footer Section -->
  <Footer/>
</Layout>
